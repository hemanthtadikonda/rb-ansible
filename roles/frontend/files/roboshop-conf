proxy_http_version 1.1;
location /images/ {
  expires 5s;
  root   /usr/share/nginx/html;
  try_files $uri /images/placeholder.jpg;
}
location /api/catalogue/ { proxy_pass http://catalogue-qa.tadikonda.online:80/; }
location /api/user/ { proxy_pass http://user-qa.tadikonda.online:80/; }
location /api/cart/ { proxy_pass http://cart-qa.tadikonda.online:80/; }
location /api/shipping/ { proxy_pass http://shipping-qa.tadikonda.online:80/; }
location /api/payment/ { proxy_pass http://payment-qa.tadikonda.online:80/; }

location /health {
  stub_status on;
  access_log off;
}